module Bar where

template Bar
    with
        owner : Party
    where
        signatory owner

data ParameterizedContractId a = ParameterizedContractId
    with
        parameterizedContractId: ContractId a
  deriving (Eq, Show)

template Haha
    with
        p : ParameterizedContractId Bar
        owner : Party
    where
        signatory owner

