
{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2015"
     ],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": ".",
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true
    },
  "include": ["**/*.ts"]
}
