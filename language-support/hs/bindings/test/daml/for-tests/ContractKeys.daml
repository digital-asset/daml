module ContractKeys where

data Key =
  Key
    with
  p : Party
  k : Int

template WithKey
  with
    owner : Party
    n : Int
  where
    signatory owner
    key Key owner n : Key
    maintainer key.p

template WithoutKey
  with
    owner : Party
    n : Int
  where
    signatory owner
