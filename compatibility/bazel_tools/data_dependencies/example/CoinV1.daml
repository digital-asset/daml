-- COIN_BEGIN
module CoinV1 where

template Coin
  with
    issuer : Party
    owner : Party
  where
    signatory issuer, owner
-- COIN_END

template CoinProposal
  with
    issuer : Party
    owner : Party
  where
    signatory issuer
    observer owner
    choice CoinProposal_Accept : ContractId Coin
      controller owner
      do create Coin with ..
