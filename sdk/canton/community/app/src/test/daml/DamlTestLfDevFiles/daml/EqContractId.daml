module EqContractId where

import Daml.Script
import DA.Assert

template Foo with
    p: Party
  where
  signatory p

main = script do
  alice <- allocateParty "Alice"
  cid1 <- submit alice do createCmd Foo{p = alice}
  cid2 <- submit alice do createCmd Foo{p = alice}
  cid1 === cid1
  cid1 =/= cid2
