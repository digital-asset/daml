module Bar where

import IBar
import Util

template Bar
  with
    owner: Party
    obs: Party
  where
    signatory owner
    observer obs

    interface instance IBar for Bar where
      view = EmptyView
      owner = owner
      choiceBody informee =
        do
          _ <- create Bar with owner = owner, obs = informee
          let just3 = id 3
          pure ()
