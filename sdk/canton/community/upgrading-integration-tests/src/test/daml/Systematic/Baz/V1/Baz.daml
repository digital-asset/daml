module Baz where

import IBaz
import Util

template Baz
  with
    owner: Party
    obs: Party
  where
    signatory owner
    observer obs

    interface instance IBaz for Baz where
      view = EmptyView
      owner = owner
      choiceBody informee =
        do
          _ <- create Baz with owner = owner, obs = informee
          let just3 = id 3
          pure ()
