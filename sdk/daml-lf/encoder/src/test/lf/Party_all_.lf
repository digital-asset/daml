module PartyMod {

  record @serializable Box = { x: Party, party: Party } ;

  template (this : Box) =  {
    precondition True;
    signatories (PartyMod:one (PartyMod:Box {party} this));
    observers (Nil @Party);
  };

  val one: Party -> List Party =
    \(x: Party) -> Cons @Party [x] (Nil @Party);

}
