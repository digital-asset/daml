module TimestampMod {

  record @serializable Box = { x: Timestamp, party: Party  } ;

  template (this : Box) =  {
    precondition True;
    signatories (PartyMod:one (TimestampMod:Box {party} this));
    observers (Nil @Party);
  };

}
