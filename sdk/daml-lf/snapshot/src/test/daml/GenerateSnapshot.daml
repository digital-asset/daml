module GenerateSnapshot where

import Daml.Script
import ReplayBenchmark

generateSnapshot = do
  alice <- allocateParty "alice"
  _ <- submit alice (createAndExerciseCmd (T alice 42) (Add 7))
  pure ()

explode = do
  alice <- allocateParty "alice"
  _ <- submit alice (createAndExerciseCmd (T alice 17) Explode)
  pure ()
