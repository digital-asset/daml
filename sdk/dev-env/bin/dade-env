#!/usr/bin/env bash
set -euo pipefail

if [[ $# -lt 1 ]]; then
   cat <<EOF
Usage: $0 <cmd>

Execute 'cmd' in a shell with dev-env sourced.

EOF
  exit 1
fi

DADE_CURRENT_SCRIPT_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
eval "$("${DADE_CURRENT_SCRIPT_DIR}/../lib/dade-dump-profile")"

exec "$@"
