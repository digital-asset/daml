module ExplicitSerializable.Imported where

data SeriImported = SeriImported with
  foo : Int
  bar : Party
    deriving (Eq, Show)

instance Serializable SeriImported

data SeriImportedHOT a = SeriImportedHOT with
  foo : a
  bar : Party
    deriving (Eq, Show)

instance Serializable a => Serializable (SeriImportedHOT a)
