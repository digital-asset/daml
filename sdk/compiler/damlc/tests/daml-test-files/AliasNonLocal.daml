-- Check that aliases for-top level functions and constructors work.

module AliasNonLocal where

import DA.Assert

data Coin = BTC | Other Text
  deriving (Eq, Show)

btc = BTC

other = Other

main = scenario do
  let btc1 = btc
  let btc2 = BTC
  let other1 = other
  let other2 = Other
  btc1 === btc2
  other1 "ETH" === other2 "ETH"

-- @ENABLE-SCENARIOS
