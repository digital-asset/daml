module Main where

-- alpha-equivalent to v1 after synonym normalization
data TKey b = TKey { p : Party, q : NonUniformSyn b }
type NonUniformSyn c = NonUniform c
data NonUniform d = Nil | NonUniform (TKey (d, d))

template T with
    p: Party
  where
  signatory p
  key (TKey p Nil) : TKey Int
  maintainer key.p
