module Main where

data TKey a = TKey { p : Party, q : NonUniform a }
data NonUniform a = Nil | NonUniform (TKey (a, a))

template T with
    p: Party
  where
  signatory p
  key (TKey p Nil) : TKey Int
  maintainer key.p
