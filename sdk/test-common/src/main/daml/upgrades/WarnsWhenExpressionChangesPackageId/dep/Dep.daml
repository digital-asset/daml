module Dep (PartyWrapper(..), unwrapPartyWrapper) where

data PartyWrapper = PartyWrapper { p: Party }
  deriving (Show, Eq)

unwrapPartyWrapper : PartyWrapper -> Party
unwrapPartyWrapper (PartyWrapper p) = p

