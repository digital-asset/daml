module Main where

data MyVariant
  = MyVariantCons1 { field1: Text }
  | MyVariantCons2 { field1: Text, field2: Text }
  | MyVariantCons3 { field1: Text, field2: Text, field3: Text }
  deriving (Show, Eq)

template A with
    p : Party
  where
    signatory p
    choice C : ()
      with
        choiceArg : MyVariant
      controller p
      do pure ()

