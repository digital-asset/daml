module Main where

import qualified Dep

data D = D { field1 : Text }

template T with
    p: Party
    t: ContractId Dep.T
  where
    signatory p
    choice C1 : D
      controller p
      do
        depD <- exercise t Dep.C1
        pure $ D "field1"
    choice C2 : Dep.D
      controller p
      do exercise t Dep.C1
