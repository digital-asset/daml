module Dep where

import qualified DepDep

data DepDepWrapper = DepDepWrapper DepDep.D
data D = D { field1 : Text, depDep : DepDep.D }

template T with
    p: Party
    t: ContractId DepDep.T
  where
    signatory p
    choice C1 : D
      controller p
      do
        depDepD <- exercise t DepDep.C1
        pure $ D "field1" depDepD

