module DepDep where

data D = D { field1 : Text, field2 : Optional Text }

template T with
    p: Party
  where
    signatory p
    choice C1 : D
      controller p
      do pure $ D "field1" (Some "field2")
    choice C2 : D
      controller p
      do pure $ D "field1" None

