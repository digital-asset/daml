module MyTemplates where

template ValidUpgrade
  with
    party : Party
  where
    signatory party

    nonconsuming choice Noop : () with
        ctl : Party
      controller ctl
      do pure ()

template ValidUpgradeUtils
  with
    party : Party
  where
    signatory party

    choice ValidUpgradeFetch : ValidUpgrade with
        cid : ContractId ValidUpgrade
      controller party
      do fetch cid
