-- this test will no longer make sense.

module InterfaceRetro where

import Interfaces (T2 (..))

data I5View = I5View {}

interface I5 where
  viewtype I5View
  getOwner5 : Party
  choice C5 : ()
    controller getOwner5 this
    do pure ()

  interface instance I5 for T2 where
    view = I5View
    getOwner5 = owner2
