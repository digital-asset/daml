daml 1.2

module Examples where

nfib 0 = 1
nfib 1 = 1
nfib n = nfib (n-1) + nfib (n-2) + 1

triangle 0 = 0
triangle n = n + triangle (n-1)

triangleTR = triangleTR_acc 0
triangleTR_acc acc 0 = acc
triangleTR_acc acc n = triangleTR_acc (acc+n) (n-1)
