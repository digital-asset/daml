module OptionMod {

  record @serializable Box = { x: Option Int64, party: Party } ;

  template (this : Box) =  {
    precondition True,
    signatories (PartyMod:one (OptionMod:Box {party} this)),
    observers (Nil @Party),
    agreement "Agreement for OptionMod:Box",
    choices {
    }
  };

}
