module BoolMod {

  record @serializable Box = { x: Bool, party: Party } ;

  template (this : Box) =  {
    precondition True;
    signatories (PartyMod:one (BoolMod:Box {party} this));
    observers (Nil @Party);
    agreement "Agreement for BoolMod:Box";
  };

}
