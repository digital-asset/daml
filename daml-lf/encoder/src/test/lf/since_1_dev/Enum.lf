module Enum {

  enum Color = Red | Green | Blue;

  record @serializable Box = { x: Enum:Color, party: Party } ;

  template (this : Box) =  {
    precondition True,
    signatories (Party:one (Enum:Box {party} this)),
    observers (Nil @Party),
    agreement "Agreement",
    choices {
    }
  };

}