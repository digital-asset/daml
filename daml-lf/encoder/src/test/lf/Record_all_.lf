module Record {

  record @serializable Pair (a:*) (b: *) = { fst: a, snd: b };

  record @serializable Box = { x: Record:Pair Int64 (Numeric 10), party: Party } ;

  template (this : Box) =  {
    precondition True,
    signatories (Party:one (Record:Box {party} this)),
    observers (Nil @Party),
    agreement "Agreement",
    choices {
    }
  };

}