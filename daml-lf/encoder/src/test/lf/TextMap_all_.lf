module TextMapMod {

  record @serializable Box = { x: TextMap Int64, party: Party } ;

  template (this : Box) =  {
    precondition True;
    signatories (PartyMod:one (TextMapMod:Box {party} this));
    observers (Nil @Party);
    agreement "Agreement TextMapMod:Box";
  };

}
