module DateMod {

  record @serializable Box = { x: Date, party: Party  } ;

  template (this : Box) =  {
    precondition True;
    signatories (PartyMod:one (DateMod:Box {party} this));
    observers (Nil @Party);
    agreement "Agreement for DateMod:Box";
  };

}
