module Decimal {

  record @serializable Box = { x: Numeric 10, party: Party  } ;

  template (this : Box) =  {
    precondition True,
    signatories (Party:one (Decimal:Box {party} this)),
    observers (Nil @Party),
    agreement "Agreement for Decimal:Box",
    choices {
    }
  };

}