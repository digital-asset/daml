module UnitMod {

  record @serializable Box = { x: Unit, party: Party  } ;

  template (this : Box) =  {
    precondition True;
    signatories (PartyMod:one (UnitMod:Box {party} this));
    observers (Nil @Party);
    agreement "Agreement for UnitMod:Box";
  };

}
