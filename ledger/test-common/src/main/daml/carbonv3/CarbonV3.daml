module CarbonV3 where

import CarbonV2

data RetroView = RetroView with
  value : Int

interface RetroI where
  viewtype RetroView
  getA: Int
  interface instance RetroI for CarbonV2.T where
    getA = a
    view = RetroView with
      value = a
