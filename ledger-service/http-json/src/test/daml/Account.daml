daml 1.2
module Account where

data AccountStatus = Enabled Time | Disabled Time
  deriving (Eq, Show)

type AccountKey = (Party, Text)

template Account with
    owner : Party
    number : Text
    status : AccountStatus
  where
    signatory owner

    key (owner, number) : AccountKey
    maintainer key._1
