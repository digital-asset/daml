{-# LANGUAGE Rank2Types #-}
{-# OPTIONS_GHC -Wno-type-defaults #-}

daml 1.2
module Rank2 where

applyBoth : (forall c . [c] -> [c]) -> ([a], [b]) -> ([a], [b])
applyBoth f (a,b) = (f a, f b)

main = scenario do
    alice <- getParty "alice"
    submit alice $ assert $ applyBoth reverse ([1,2,3],["a","b"]) == ([3,2,1],["b","a"])
