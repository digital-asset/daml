daml 1.2
module PartyCompare where

test = scenario do
  alice <- getParty "Alice"
  bob <- getParty "Bob"
  assert $ alice < bob
  assert $ alice >= alice
  assert $ bob >= alice
  assert $ alice <= alice
  assert $ alice <= bob
  assert $ bob > alice
  assert $ bob >= bob
  assert $ bob >= alice
  assert $ alice < bob
  assert $ bob > alice
  assert $ alice <= bob
  assert $ bob <= bob
