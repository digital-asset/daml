module PartyCompare where

test = scenario do
  alice <- getParty "Alice"
  bob <- getParty "Bob"
  assert $ alice < bob
  assert $ not (alice < alice)
  assert $ not (bob < alice)
  assert $ alice <= alice
  assert $ alice <= bob
  assert $ not (bob <= alice)
  assert $ bob >= bob
  assert $ bob >= alice
  assert $ not (alice >= bob)
  assert $ bob > alice
  assert $ not (alice > bob)
  assert $ not (bob > bob)
