module MangledScenario' where

import Daml.Script

template T'
  with
    p : Party
  where
    signatory p

data NestedT
  = T1 { t1 : Int }
  | T2 { t2 : Int }

mangled' = script do
  alice <- allocateParty "Alice"
  t' <- submit alice (createCmd T' with p = alice)
  submit alice (exerciseCmd t' Archive)
  pure (T1 0)
