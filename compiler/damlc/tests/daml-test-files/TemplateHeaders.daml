-- Test parsing for "template headers", i.e. the types between `template` and `with`.

module TemplateHeaders where

template A with
    pa : Party
  where
    signatory pa
    controller pa can
      -- Used in template I
      Transfer : ContractId A
        with newParty: Party
        do create A with pa = newParty

template () => D
  with
    pd: Party
  where
    signatory pd
