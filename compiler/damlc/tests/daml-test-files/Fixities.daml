module Fixities where

data Pair a b = Pair with
  fst : a
  snd : b
infixr 5 `Pair`

pair : a -> b -> Pair a b
pair = Pair
infixr 5 `pair`

class Category cat where
  id : cat a a
  (<<<) : cat b c -> cat a b -> cat a c
  infixr 1 <<<

class Category a => Arrow a where
  (&&&) : a b c -> a b c' -> a b (c,c')
  infixr 3 &&&
