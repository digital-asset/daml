-- Check that single constructor enum types can be exported like other types.
-- @SINCE-LF 1.6
-- @QUERY-LF .modules[] | .data_types[] | select(lf::get_dotted_name($pkg) == ["Color"]) | has("enum")
daml 1.2
module SingleConEnumExport ( Color(..), Weather(Sunny) ) where

data Color = Red
    deriving (Eq, Show)

data Weather = Sunny
    deriving (Eq, Show)
