module MonoidTest where

import DA.Assert
import DA.Monoid

testSum = scenario do
  Sum (1 : Int) <> Sum 2 === Sum (1 + 2)
  mempty === Sum (aunit : Int)

testProduct = scenario do
  Product (2 : Int) <> Product 3 === Product (2 * 3)
  mempty === Product (munit : Int)

