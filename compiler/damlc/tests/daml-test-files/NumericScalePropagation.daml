-- Test that numeric scale information propagates automatically when working
-- with numeric literals.
--
-- @SINCE-LF 1.7



module NumericScalePropagation where

scalePropWhere : Numeric 10
scalePropWhere = a + b
    where
        a = 0.1
        b = 0.2

scalePropLet : Numeric 15
scalePropLet =
    let x = 0.1
        y = 0.2
    in x*x + y*y
