module Self where

import DA.Assert

template Self with
    p: Party
  where
  signatory p

  choice Same : ()
    with other: ContractId Self
    controller p
    do self === other

main = scenario do
  alice <- getParty "Alice"
  submit alice do
    cid1 <- create Self with p = alice
    exercise cid1 Same with other = cid1
