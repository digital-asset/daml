-- @SINCE-LF-FEATURE DAML_INTERFACE
-- @ENABLE-SCENARIOS
-- @ERROR range=26:7-26:20; Multiple declarations of method ‘m0’ in template ‘T’ implementation of interface ‘I’

module InterfaceMultipleMethodDeclsError where

interface I where
  viewtype ()
  m0 : Bool -> Party
  m1 : Text

template T
  with
    p0 : Party
    p1 : Party
    f : Int
  where
    signatory p0, p1

    implements I where
      view = ()
      -- first m0 declaration
      m0 False = p0
      m1 = "m1"
      -- second m0 declaration
      m0 True = p1
