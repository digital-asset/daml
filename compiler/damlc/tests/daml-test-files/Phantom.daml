module Phantom where

import DA.Assert

data Phantom a = Phantom {}
    deriving (Eq, Show)
phantom : Phantom (Party -> Party)
phantom = Phantom {}

showOptional : (a -> Text) -> Optional a -> Text
showOptional f x = optional "None" f x

main = scenario do
  phantom === Phantom {}
  showOptional show (None : Optional Int) === "None"
