module TextEq where -- test of DEL-3881
import Daml.Script
import DA.Assert

stringEdit x = case x of
  "USD" -> "JPY"
  "JPY" -> "USD"
  x -> x

main = script do
    stringEdit "USD" === "JPY"
    stringEdit "JPY" === "USD"
    stringEdit "Daml" === "Daml"
