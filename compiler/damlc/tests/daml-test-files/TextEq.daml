module TextEq where -- test of DEL-3881

stringEdit x = case x of
  "USD" -> "JPY"
  "JPY" -> "USD"
  x -> x

main = scenario do
    alice <- getParty "alice"
    submit alice $ assert $ stringEdit "USD" == "JPY"
    submit alice $ assert $ stringEdit "JPY" == "USD"
    submit alice $ assert $ stringEdit "DAML" == "DAML"
