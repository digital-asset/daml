daml 1.2
module NewtypeDerive where

newtype New a = New a deriving (Eq, Ord, Show, Functor)

main = scenario do
  assert $ New "x" == New "x"
  assert $ New 1 < New 2
  assert $ show (New 1) == "New 1"
  assert $ fmap (+1) (New 1) == New 2
