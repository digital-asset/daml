module EqContractId where

template Foo with
    p: Party
  where
  signatory p

main = scenario do
  alice <- getParty "Alice"
  cid1 <- submit alice do create Foo{p = alice}
  cid2 <- submit alice do create Foo{p = alice}
  assert $ cid1 == cid1
  assert $ cid1 /= cid2
