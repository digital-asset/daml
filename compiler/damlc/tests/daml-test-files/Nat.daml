module Nat where

import DA.Assert

data Nat = Z | S Nat
      deriving (Eq,Ord,Show)


add : Nat -> Nat -> Nat
add Z x = x
add (S x) y = add x (S y)

main = scenario do
    add (S (S Z)) (S Z) === S (S (S Z))
