-- @ERROR Aborted: BOOM

module OverAppliedFoldEvalOrder where

boom = scenario do
  let id x = x
  let boomCompose _ _ = error "BOOM"
  let _ = foldr boomCompose id [id] (error "BANG")
  pure ()
