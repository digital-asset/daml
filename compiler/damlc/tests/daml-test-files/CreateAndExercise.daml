module CreateAndExercise where

import DA.Assert

template T1
  with
    p : Party
  where
    signatory p
    choice C1 : Int
      controller p
      do pure 42

main = scenario do
  alice <- getParty "Alice"
  r <- alice `submit` createAndExercise (T1 alice) C1
  r === 42
