{-# OPTIONS_GHC -Wincomplete-patterns #-}
-- DEL-4670: This used to erroneously warn about non-exhaustive pattern matches.

module Otherwise where

warning : Bool -> [a] -> [a]
warning _ [] = []
warning b (x :: xs')
  | b = warning b xs'
  | otherwise = x :: xs'

warning3 : Optional a -> [a]
warning3 mX
  | Some _ <- mX = warning3 None
  | otherwise = []
