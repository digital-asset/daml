{-# LANGUAGE Rank2Types #-}
{-# OPTIONS_GHC -Wno-type-defaults #-}


module Rank2 where

import DA.Assert

applyBoth : (forall c . [c] -> [c]) -> ([a], [b]) -> ([a], [b])
applyBoth f (a,b) = (f a, f b)

main = scenario do
  applyBoth reverse ([1,2,3],["a","b"]) === ([3,2,1],["b","a"])

-- @ENABLE-SCENARIOS
