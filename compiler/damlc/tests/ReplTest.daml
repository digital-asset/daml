module ReplTest where

template T
  with
    proposer : Party
    accepter : Party
  where
    signatory proposer, accepter

template TProposal
  with
    proposer : Party
    accepter : Party
  where
    signatory proposer
    observer accepter
    choice Accept : ContractId T
      controller accepter
      do  create (T proposer accepter)

