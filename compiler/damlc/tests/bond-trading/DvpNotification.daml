daml 1.2
module DvpNotification
where

import DvpTerms

type DvpNotificationId = ContractId DvpNotification

template DvpNotification
  with
    c : DvpTerms
  where
    ensure isValidDvp c
    signatory c.seller

    controller c.buyer can
      Accept : ()
        do
          return ()
