-- | MOVE DA.Date
module DA.Date.Types
  ( DayOfWeek(..)
  , Month(..)
  ) where

data DayOfWeek
    = Monday
    | Tuesday
    | Wednesday
    | Thursday
    | Friday
    | Saturday
    | Sunday

-- | The `Month` type represents a month in the Gregorian calendar.
data Month
    = Jan
    | Feb
    | Mar
    | Apr
    | May
    | Jun
    | Jul
    | Aug
    | Sep
    | Oct
    | Nov
    | Dec
