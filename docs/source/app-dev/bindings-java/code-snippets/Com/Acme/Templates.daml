module Com.Acme.Templates where

data BarKey =
  BarKey
    with
      p : Party
      t : Text

template Bar
  with
    owner: Party
    name: Text
  where
    signatory owner

    key BarKey owner name : BarKey
    maintainer key.p
    
    controller owner can
      Bar_SomeChoice: Bool
        with
        aName: Text
          do return True
-- end snippet: template example
